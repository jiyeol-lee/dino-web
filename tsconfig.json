{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "preserveValueImports": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/global-types": [
        "./src/types/index.ts"
      ],
      "@/global-types/*": [
        "./src/types/*"
      ],
      "@/components/*": [
        "./src/components/*"
      ],
      "@/pages/*": [
        "./src/pages/*"
      ],
      "@/hooks/*": [
        "./src/hooks/*"
      ],
      "@/utils": [
        "./src/utils/index.ts"
      ],
      "@/utils/*": [
        "./src/utils/*"
      ],
      "@/models/*": [
        "./src/models/*"
      ],
      "@/redux-store": [
        "./src/redux/store"
      ],
      "@/redux-types": [
        "./src/redux/types"
      ],
      "@/redux-types/*": [
        "./src/redux/types/*"
      ],
      "@/redux-action-creators": [
        "./src/redux/action-creators"
      ]
    }
  },
  "include": ["next-env.d.ts", "environment.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
